---
const response = await fetch('https://dummyjson.com/users/');
const data = await response.json();
const users = data.users;
---

<!-- Data fetched at build can be rendered in HTML -->
<h2>User</h2>
<ul>
  {users.map(user => (
    <li>{user.firstName} {user.lastName} ({user.email})</li>
  ))}
</ul>